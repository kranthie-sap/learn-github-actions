{
  "name": "@sap-ux/weather-core",
  "private": false,
  "version": "0.0.5",
  "dependencies": {
    "dotenv": "10.0.0",
    "openweathermap-ts": "1.2.7"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write '**/*.{css,scss,html,js,json,ts,tsx,yaml,yml}' '!**/{out,dist,typings,node_modules}/**' '!**/*.{svg,png,xml}' --ignore-path ../../../.prettierignore",
